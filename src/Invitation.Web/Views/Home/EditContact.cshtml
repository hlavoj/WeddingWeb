@model Contact
@{
    ViewData["Title"] = "EditContact";
}
<div class="editContact">
    <br />
    <br/>
    <form method="post" asp-controller="Home" asp-action="WantProgram">
        <div class="form-group">
            <label for="exampleInputEmail1">Email</label>
            <input type="email" asp-for="Email" class="form-control" id="exampleInputEmail1" placeholder="Email">
        </div>
        <br />
        <div class="form-group">
            <label for="exampleInputPassword1">Telefon</label>
            <input type="number" asp-for="Number" class="form-control" id="exampleInputPassword1" placeholder="Telefon">
        </div>
        <div class="text-center">
            <button type="submit" class="btn btn-danger btn-lg">Hotovo</button>
        </div>
    </form>
    <br />
    <br/>
   
</div>



